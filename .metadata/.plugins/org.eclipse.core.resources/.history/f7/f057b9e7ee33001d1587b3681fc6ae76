package array;

public class Test {

	public static void main(String[] args) {
		
		int money = 2600;
		
		int[] arr = {500,100,50,10};
		
		int num1 = money % arr[0];
		
		
		for(int i=0;i<arr.length;i++) {
				System.out.println("hint :" + money);
				if(num1 >= 1) {
				num1=money / arr[i];
			}
			System.out.println(num1);
		}
		
		
	
		
		
	}

}
