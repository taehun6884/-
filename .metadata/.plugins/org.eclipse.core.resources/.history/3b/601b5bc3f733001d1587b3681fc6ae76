package variable_arguments;

public class Test {

	public static void main(String[] args) {
		
		PtintMethod pt = new PtintMethod();
		
		pt.print();
		
	}

}

class PtintMethod{
	
//	public void print() {
//		System.out.println("전달 받은 정수 없음!");
//	}

	public void print(int...num) {
		for(int i=0;i<num.length;i++) {
		if(num.length == 0) {
			System.out.println("전달 받은 정수 없음!");
			}
		System.out.print(num[i]+ " ");
		}

	
	
	
	}
}
